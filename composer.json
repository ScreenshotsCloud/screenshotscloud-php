{
	"name": "screenshotscloud/screenshotscloud",
	"version": "1.0.1",
	"description": "ScreenshotsCloud PHP screenshot generator",
	"type": "library",
	"authors": [{
		"name": "Chris Hutchinson",
		"email": "chris@brushd.com"
	}],
	"keywords": [
		"screenshots",
		"screenshots.cloud",
		"firefox",
		"browser"
	],
	"autoload": {
		"psr-4": {
			"ScreenshotsCloud\\": "src/"
		}
	},
    "autoload-dev": {
        "psr-4": {
            "ScreenshotsCloud\\Tests\\": "tests/"
        }
    },
	"require": {
		"php": ">=5.3.0"
	},
	"license": "MIT",
	"require-dev": {
		"phpunit/phpunit": "5.7"
	}
}
